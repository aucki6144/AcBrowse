<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.aucki.acbrowse">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/app_launcher_1"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/app_launcher_1_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AcBrowse"
        android:usesCleartextTraffic="true">
        <activity
            android:name=".activity.SetHomepageActivity"
            android:theme="@style/MyDialogTheme"
            android:exported="false" />
        <activity
            android:name=".activity.UrlListActivity"
            android:exported="false" />
        <activity
            android:name=".activity.SettingsActivity"
            android:exported="false" />
        <activity
            android:name=".activity.BrowseActivity"
            android:exported="true"
            android:theme="@style/MainActivityTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>